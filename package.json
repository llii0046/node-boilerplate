{
  "name": "ts-express-prisma-boilerplate",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/src/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:logger": "tsx test-logger-output.ts",
    "log:console": "node scripts/setup-logging.js console",
    "log:file": "node scripts/setup-logging.js file",
    "log:both": "node scripts/setup-logging.js both",
    "test:file-logging": "tsx test-file-logging.ts",
    "test:validation": "tsx test-validation-new.ts",
    "db:start": "docker-compose up -d db",
    "db:stop": "docker-compose down",
    "db:restart": "docker-compose restart db",
    "db:logs": "docker-compose logs -f db",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "pino": "^9.0.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2",
    "save": "^2.9.0",
    "swagger-ui-express": "^5.0.1",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.9.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "husky": "^9.1.6",
    "jest": "^30.0.5",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  }
}
